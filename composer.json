        "yiisoft/log": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.2",
