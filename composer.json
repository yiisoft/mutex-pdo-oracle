{
    "name": "yiisoft/mutex-db-oracle",
    "type": "library",
    "description": "Yii Mutex Library - Oracle DB Driver",
    "keywords": [
        "yii",
        "mutex"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Zlakomanov",
            "email": "zlakomanoff@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/yiisoft/mutex-db-oracle",
        "issues": "https://github.com/yiisoft/mutex-db-oracle/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "require": {
        "yiisoft/mutex": "^3.0@dev",
        "ext-pdo": "*"
    },
    "require-dev": {
